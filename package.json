    "connect-redis": "^6.1.3",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "helmet": "^4.6.0",
    "husky": "^7.0.2",
    "jest": "^28.1.3",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "pg": "^8.7.1",
    "pg-mem": "^2.6.3",
    "pg-promise": "^10.11.1",
