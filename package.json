  "license": "MIT",
  "main": "dist/main.js",
  "name": "my-financial-monitor",
  "scripts": {
    "build": "rm -fr ./dist && npm ci && tsc && chmod u+x dist/cli.js",
    "check-types": "tsc --noEmit --project ./tsconfig.json",
    "cli": "node --require dotenv/config dist/cli.js",
    "dev": "nodemon --config nodemon.dev.json",
    "format": "prettier src --write",
    "lint": "eslint src --ext .js,.ts --fix",
    "postbuild": "node utilities/format-query-files.js && node utilities/copy-query-files.js",
    "prepare": "husky install",
    "x": "node --require dotenv/config dist/main.js",
    "start": "nodemon",
    "test": "DOTENV_CONFIG_PATH=./.env.test NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.json --setupFiles dotenv/config"
